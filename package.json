{
  "name": "ngin_client",
  "version": "0.2.9",
  "description": "Node client for interacting with the NGIN API services",
  "main": "./index.js",
  "dependencies": {
    "glob": "*",
    "request": "*",
    "underscore": "*",
    "extendable": "git://github.com/mikefrey/extendable-1"
  },
  "devDependencies": {
    "mocha": "*",
    "sinon": "*",
    "blanket": "*"
  },
  "engines": {
    "node": ">=0.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sportngin/ngin_client_node"
  },
  "scripts": {
    "test": "mocha --recursive -R spec",
    "coverage": "mocha --recursive --require blanket -R html-cov > coverage.html && open ./coverage.html",
    "blanket": {
      "pattern": "src"
    }
  }
}
